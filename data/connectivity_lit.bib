@article{ ISI:000407895000045,
Author = {Correa, Alicia and Windhorst, David and Tetzlaff, Doerthe and Crespo,
   Patricio and Celleri, Rolando and Feyen, Jan and Breuer, Lutz},
Title = {{Temporal dynamics in dominant runoff sources and flow paths in the
