@article{ ISI:000388173000014,
Author = {He, Dekui and Kang, Zujie and Tao, Juan and Liu, Chunlong and Yang, Jian
   and Chen, Yifeng},
Title = {{Hydrologic connectivity driven natural stream fish assemblages in
   mountain streams in the Yangtze River basin: implications for stream
