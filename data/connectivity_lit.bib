@article{ ISI:000323041600001,
Author = {Castello, Leandro and McGrath, David G. and Hess, Laura L. and Coe,
   Michael T. and Lefebvre, Paul A. and Petry, Paulo and Macedo, Marcia N.
   and Reno, Vivian F. and Arantes, Caroline C.},
Title = {{The vulnerability of Amazon freshwater ecosystems}},
Journal = {{CONSERVATION LETTERS}},
Year = {{2013}},
Volume = {{6}},
Number = {{4}},
Pages = {{217-229}},
Month = {{JUL}},
Abstract = {{The hydrological connectivity of freshwater ecosystems in the Amazon
   basin makes them highly sensitive to a broad range of anthropogenic
   activities occurring in aquatic and terrestrial systems at local and
   distant locations. Amazon freshwater ecosystems are suffering escalating
   impacts caused by expansions in deforestation, pollution, construction
   of dams and waterways, and overharvesting of animal and plant species.
   The natural functions of these ecosystems are changing, and their
   capacity to provide historically important goods and services is
   declining. Existing management policiesincluding national water
   resources legislation, community-based natural resource management
   schemes, and the protected area network that now epitomizes the Amazon
   conservation paradigmcannot adequately curb most impacts. Such
   management strategies are intended to conserve terrestrial ecosystems,
   have design and implementation deficiencies, or fail to account for the
   hydrologic connectivity of freshwater ecosystems. There is an urgent
   need to shift the Amazon conservation paradigm, broadening its current
   forest-centric focus to encompass the freshwater ecosystems that are
   vital components of the basin. This is possible by developing a river
   catchment-based conservation framework for the whole basin that protects
   both aquatic and terrestrial ecosystems.}},
DOI = {{10.1111/conl.12008}},
Keywords = {{Conservation; ecosystem goods and services; floodplain; hydrologic
   connectivity; policy; protected areas; wetlands}},
Keywords-Plus = {{SCALE FISHERIES MANAGEMENT; LATERAL MIGRATION; BRAZILIAN AMAZON;
   EMERGING SCIENCE; PROTECTED AREAS; SOUTH-AMERICA; RIVER; FLOODPLAIN;
   BASIN; DEFORESTATION}},
