@article{ ISI:000346398600012,
Author = {Mosquera, Giovanny M. and Lazo, Patricio X. and Celleri, Rolando and
   Wilcox, Bradford P. and Crespo, Patricio},
Title = {{Runoff from tropical alpine grasslands increases with areal extent of
   wetlands}},
Journal = {{CATENA}},
Year = {{2015}},
Volume = {{125}},
Pages = {{120-128}},
Month = {{FEB}},
Abstract = {{Tropical alpine grasslands of the northern Andes, commonly known as the
   ,Aram, provide abundant highquality water for downstream populations as
   well as a variety of other environmental services. Yet, very little is
   known about the role that landscape characteristics play in the
   hydrologic functioning of these ecosystems. To help fill this knowledge
   gap, we investigated the relationships between various landscape
   attributes and hydrology in a wet paramo ecosystem of southern Ecuador.
   Using linear regression analysis, we examined the influence of soil
   type, vegetation cover, catchment area, geology, and topography on
   runoff coefficient, streamflow rates, and evapotranspiration. Our study
   site is located at the Zhurucay River experimental catchment, which is
   composed of seven nested microcatchments ranging in size from 0.20 to
   7.53 km(2) and in elevation from 3200 to 3900 m We found that (1) water
   yield accounts for a high percentage of the water budget; (2) runoff
   coefficient and specific discharge are strongly correlated with the
   extent of Andean paramo wetlands (Histosol soils and cushion plants),
   and also increase with catchment size; (3) conversely, inferred
   evapotranspiration is the highest in catchments having the greatest
   percentages of upland terrain (Andosol soils and tussock grasses); and
   (4)low flows are highly positively correlated with steep slopes. These
   results suggest that in the high-elevation tropical grasslands of the
   Andes, runoff coefficient and specific discharge increase with catchment
   size because as catchment size increases, so does the relative area of
   permanently near-saturation zones (Andean paramo wetlands); likely
   because of reduced available storage capacity of the Andean `Aram
   wetlands, and their hydrologic connectivity to the stream network. (C)
   2014 Elsevier B.V. All rights reserved.}},
DOI = {{10.1016/j.catena.2014.10.010}},
Keywords = {{Andean Paramo wetlands; Hydrologic processes; Topography; Andosol and
   Histosol; Ecuador; Andes}},
Keywords-Plus = {{ECUADORIAN PARAMO; STORAGE DYNAMICS; CLIMATE-CHANGE; WATER YIELD;
   CATCHMENT; ANDES; CONNECTIVITY; STREAMFLOW; SCALE; GENERATION}},
Web-of-Science-Categories  = {{Geosciences, Multidisciplinary; Soil Science; Water Resources}},
