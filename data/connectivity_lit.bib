}

@article{ ISI:000360769300011,
Author = {Lasne, E. and Sabatie, M. -R. and Jeannot, N. and Cucherousset, J.},
Title = {{The Effects of DAM Removal on River Colonization by Sea Lamprey
   Petromyzon Marinus}},
Journal = {{RIVER RESEARCH AND APPLICATIONS}},
Year = {{2015}},
Volume = {{31}},
Number = {{7}},
Pages = {{904-911}},
Month = {{SEP}},
Abstract = {{Habitat fragmentation is an important cause of biodiversity loss in
   freshwater systems, as worldwide rivers have been fragmented by dams and
   other hydraulic structures. To restore freshwater fish populations, some
   barriers have been removed, but the long-term ecological effects of this
   removal have been rarely quantified. In the present study, we quantified
   the effects of barrier removal on river colonization by anadromous sea
   lamprey (Petromyzon marinus) by analyzing the spatial distribution and
   nest density in a small coastal river (France) from 1994 to 2011. Our
   results demonstrated the benefit of dam removal within few years after
   restoration. Indeed, the spatial distribution of nests shifted
   significantly upstream and was more uniform throughout the river after
   removal. Our results also suggest that the spatial patterns of habitat
   colonization were affected by the density of nests, river flow and
   connectivity. Finally, although the number of nests was significantly
   higher after removal, it was not possible to clearly identify the
   contribution of intrinsic versus external factors involved in this
   pattern. Further investigations are therefore needed to quantify the
   potential subsequent effects on juvenile recruitment and the overall
   population dynamics. Copyright (c) 2014 John Wiley \& Sons, Ltd.}},
DOI = {{10.1002/rra.2789}},
Keywords = {{diadromous fish; river connectivity; habitat restoration; colonization;
   spawning}},
Keywords-Plus = {{SPAWNING MIGRATION; DIADROMOUS FISHES; LONGITUDINAL CONNECTIVITY;
   HYDROLOGIC CONNECTIVITY; NETWORK STRUCTURE; FLOW REGULATION;
   LIFE-HISTORY; STREAM; HABITAT; RESTORATION}},
Web-of-Science-Categories  = {{Environmental Sciences; Water Resources}},
ORCID-Numbers = {{Cucherousset, Julien/0000-0003-0533-9479}},
Times-Cited = {{8}},
Unique-ID = {{ISI:000360769300011}},
